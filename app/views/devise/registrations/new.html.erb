<%= simple_form_for resource, as: resource_name, url: registration_path(resource_name), html: {class: 'form-inline' }, method: :post  do |f| %>
  <%= f.input :email, required: true, autofocus: true, label: false, placeholder: "email"%>
  <%= f.input :password, required: true, label: false, placeholder: "email" %>
  <%= f.button :submit, "Sign up", class: "log-in-btn" %>
<% end %>
